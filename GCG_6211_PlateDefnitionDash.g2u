Program.Sub.MenuClickExportAll.Start
F.Intrinsic.Control.Try
	V.Local.sError.Declare
	V.Local.sFileExport.Declare
	V.Local.bFileLocked.Declare
	V.Local.sSerialFilter.Declare
	V.Local.sXFilter.Declare
	
	F.Intrinsic.Control.If(V.DataTable.dtXStatus$dtSerialization.RowCount,=,0)
		'No records in child table, exit
		F.Intrinsic.Control.ExitSub
	F.Intrinsic.Control.EndIf
	
	Gui.frmXStatus.GsGCXStatus.GetGridviewProperty("gvXStatus","ActiveFilterString",V.Local.sXFilter)
	Gui.frmXStatus.GsGCXStatus.GetGridviewProperty("gvSerial","ActiveFilterString",V.Local.sSerialFilter)
	F.Intrinsic.Control.If(V.Local.sXFilter,=,"","and",V.Local.sSerialFilter,!=,"")
		V.Local.sXFilter.Set(V.Local.sSerialFilter)
	F.Intrinsic.Control.ElseIf(V.Local.sXFilter,!=,"","and",V.Local.sSerialFilter,!=,"")
		F.Intrinsic.String.Build("({0}) and ({1})",V.Local.sXFilter,V.Local.sSerialFilter,V.Local.sXFilter)
	F.Intrinsic.Control.EndIf
	F.Data.Linq.Join("LeftJoin","DataTable","dtXStatus$dtSerialization*!*CHILD","DataTable","dtXStatus*!*PARENT","PARENT.PART = CHILD.PART and PARENT.LOCATION = CHILD.LOCATION","*",,,"CHILD.PART desc, CHILD.LOCATION desc, CHILD.BIN desc, CHILD.SERIAL_NUMBER desc","dtExportAll",False)
	'Remove Duplicates
	F.Data.DataTable.RemoveColumn("dtExportAll","PART1")
	F.Data.DataTable.RemoveColumn("dtExportAll","LOCATION1")
	F.Data.DataTable.RemoveColumn("dtExportAll","BIN")
	
	F.Data.DataView.Create("dtExportAll","dvFiltered",22,V.Local.sXFilter,)
	F.Data.DataView.ToDataTable("dtExportAll","dvFiltered","dtExportFiltered",False)
	
	'F.Data.DataTable.Merge("dtXStatus","dtExportAll",True,2)
	
	F.Intrinsic.String.Build("{0}\GCG_6211_Inv_XStatus_Ext_AllData_Export.csv",V.Caller.LocalGssTempDir,V.Local.sFileExport)
	F.Intrinsic.File.IsFileLocked(V.Local.sFileExport,V.Local.bFileLocked)
	F.Intrinsic.Control.If(V.Local.bFileLocked)
		F.Intrinsic.String.Build("File is already open. Please close and export again.{0}File: {1}",V.Ambient.NewLine,V.Local.sFileExport,V.Local.sMsg)
		F.Intrinsic.UI.Msgbox(V.Local.sMsg,"File In Use")
	F.Intrinsic.Control.Else
		F.Data.DataTable.SaveToCSV("dtExportFiltered",V.Local.sFileExport,True)
	F.Intrinsic.Control.EndIf
	F.Intrinsic.Task.ShellExec(0,"",V.Local.sFileExport,"","",1)
	
	F.Data.DataTable.Close("dtExportAll")
	F.Data.DataTable.Close("dtExportFiltered")

F.Intrinsic.Control.Catch
	F.Intrinsic.String.Build("Project: {0}{1}{1}Subroutine: {2}{1}Error Occurred {3} with description {4}{1}{1}GAB Version: {5}",V.Caller.ScriptFile,V.Ambient.Newline,V.Ambient.CurrentSubroutine,V.Ambient.ErrorNumber,V.Ambient.ErrorDescription,V.Ambient.GABVersion,V.Local.sError)
	F.Intrinsic.UI.Msgbox(V.Local.sError)
	F.Intrinsic.Control.End
F.Intrinsic.Control.EndTry
Program.Sub.MenuClickExportAll.End

