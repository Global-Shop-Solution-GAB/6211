	F.Data.DataTable.CreateFromSQL("dtNotes","con","select PART, LOCATION, rtrim(SERIAL) as SERIAL, rtrim(BIN) as BIN, NOTES from GCG_6211_PlateNotes;")
	
	F.Data.Linq.Join("InnerJoin","DataTable","dtTemp*!*ITEM","DataTable","dtXStatus*!*STATUS","ITEM.PARTLOC = STATUS.PARTLOC","ITEM.PART*!*ITEM.LOCATION*!*ITEM.BIN*!*ITEM.SERIAL_NUMBER*!*ITEM.QUANTITY*!*ITEM.UNIT_COST",,,"ITEM.PART asc, ITEM.LOCATION asc","dtTemp2",False)
	F.Data.Linq.Join("LeftJoin","DataTable","dtTemp2*!*ITEM","DataTable","dtNotes*!*NOTES","ITEM.PART = NOTES.PART and ITEM.LOCATION = NOTES.LOCATION and ITEM.BIN = NOTES.BIN and ITEM.SERIAL_NUMBER = NOTES.SERIAL","ITEM.PART*!*ITEM.LOCATION*!*ITEM.BIN*!*ITEM.SERIAL_NUMBER*!*ITEM.QUANTITY*!*ITEM.UNIT_COST*!*NOTES.NOTES",,,"ITEM.PART asc, ITEM.LOCATION asc, ITEM.LOCATION asc","dtXStatus$dtSerialization",True)
	F.Data.DataTable.Close("dtTemp")
	F.Data.DataTable.Close("dtTemp2")
	F.Data.DataTable.Close("dtNotes")
	
	F.Data.DataTable.AddExpressionColumn("dtXStatus$dtSerialization","TOTAL_COST","Float","UNIT_COST * QUANTITY")

	F.Data.DataTable.AddRelation("dtXStatus","Part*!*Location","dtXStatus$dtSerialization","PART*!*LOCATION","rshpSerialization")
	
